<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/crosswordGrid"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:padding="1dp"
    android:background="@drawable/crossword_cell">
    <include
        android:id="@+id/row0"
        android:layout_weight="1"
        android:layout_height="0dp"
        android:layout_width="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/row1"
        layout="@layout/fragment_crossword_row" />
    <include
        android:id="@+id/row1"
        android:layout_weight="1"
        android:layout_height="0dp"
        android:layout_width="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/row0"
        app:layout_constraintBottom_toTopOf="@id/row2"
        layout="@layout/fragment_crossword_row" />
    <include
        android:id="@+id/row2"
        android:layout_weight="1"
        android:layout_height="0dp"
        android:layout_width="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/row1"
        app:layout_constraintBottom_toTopOf="@id/row3"
        layout="@layout/fragment_crossword_row" />
    <include
        android:id="@+id/row3"
        android:layout_weight="1"
        android:layout_height="0dp"
        android:layout_width="match_parent"
